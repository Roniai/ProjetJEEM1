<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="bean.Medecin" table="medecin">
	<id name="codemed" column="code_med"></id>
	
	<property name="nom" column="nom_med"></property>
	<property name="prenom" column="prenom_med"></property>
	<property name="grade" column="grade_med"></property>
	
</class>
</hibernate-mapping>